{
  "name": "aio-project-initializer",
  "version": "0.1.0",
  "description": "A CLI to initialize a project, configure & setup all the tools and do all the heavy lifting on your behalf",
  "main": "dist/index.js",
  "types": "dist/@types/index.d.ts",
  "files": [
    "assets/",
    "dist/",
    "templates/"
  ],
  "bin": {
    "aio-initialize": "dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && node_modules/.bin/tsc",
    "dev": "node_modules/.bin/tsc -w",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeeshanSharma/aio-project-initializer.git"
  },
  "author": "Deeshan Sharma <deeshansharma04@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DeeshanSharma/aio-project-initializer/issues"
  },
  "homepage": "https://github.com/DeeshanSharma/aio-project-initializer#readme",
  "dependencies": {
    "center-align": "^1.0.1",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "execa": "^5.1.1",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.2",
    "listr": "^0.14.3"
  },
  "devDependencies": {
    "@types/center-align": "^1.0.0",
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^7.3.3",
    "@types/listr": "^0.14.4",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "typescript": "^4.4.2"
  }
}
