{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-typescript/base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "import/extensions": ["off", "never"],
    "import/no-extraneous-dependencies": ["off", "never"]
  }
}
